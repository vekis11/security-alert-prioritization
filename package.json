{
  "name": "security-alert-prioritization",
  "version": "1.0.0",
  "description": "AI-powered security alert and vulnerability prioritization dashboard",
  "main": "server/index.js",
  "scripts": {
    "dev": "powershell -ExecutionPolicy Bypass -File start-dashboard.ps1",
    "server": "nodemon server/index.js",
    "client": "cd client; npm start",
    "build": "cd client; npm run build",
    "start": "node server/index.js",
    "install-all": "npm install; cd client; npm install",
    "start:server": "node server/index.js",
    "start:client": "cd client; npm start",
    "kill-all": "Get-Process -Name node -ErrorAction SilentlyContinue | Stop-Process -Force",
    "restart": "npm run kill-all; npm run dev"
  },
  "dependencies": {
    "@slack/web-api": "^6.9.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "node-cron": "^3.0.3",
    "openai": "^4.104.0",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "security",
    "vulnerability",
    "prioritization",
    "dashboard",
    "ai",
    "tenable",
    "crowdstrike",
    "veracode",
    "splunk"
  ],
  "author": "Security Team",
  "license": "MIT"
}
